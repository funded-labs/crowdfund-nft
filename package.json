{
    "name": "nextjs_ic_template",
    "version": "0.1.0",
    "author": "Henry Chan",
    "description": "Next.js Internet Computer Starter Template",
    "keywords": [
        "nextjs",
        "internet computer",
        "icp",
        "starter",
        "dfinity"
    ],
    "scripts": {
        "dev": "next dev",
        "build": "next build && next export",
        "start": "next start",
        "lint": "next lint",
        "export": "next export",
        "syncgencode": "DFX_NETWORK=local && npm run copy:types",
        "copy:types": "rsync -avr .dfx/$(echo ${DFX_NETWORK:-'**'})/canisters/** --exclude='assets/' --exclude='idl/' --exclude='*.wasm' --delete ui/declarations"
    },
    "devDependencies": {
        "autoprefixer": "10.4.0",
        "eslint": "^7.32.0",
        "eslint-config-next": "^11.0.1",
        "eslint-plugin-react": "^7.24.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "postcss": "8.4.5",
        "tailwindcss": "3.0.7"
    },
    "dependencies": {
        "@dfinity/agent": "^0.10.1",
        "@dfinity/auth-client": "^0.10.2",
        "@dfinity/authentication": "0.10.2",
        "@dfinity/candid": "^0.9.2",
        "@dfinity/identity": "0.10.2",
        "@dfinity/principal": "^0.9.2",
        "assert": "^2.0.0",
        "buffer": "^6.0.3",
        "classnames": "2.3.1",
        "events": "^3.3.0",
        "formik": "2.2.9",
        "next": "^11.0.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "stream-browserify": "^3.0.0",
        "util": "^0.12.4",
        "yup": "0.32.11"
    }
}
