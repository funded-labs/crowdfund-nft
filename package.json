{
    "name": "nextjs_ic_template",
    "version": "0.1.0",
    "author": "Henry Chan",
    "description": "Next.js Internet Computer Starter Template",
    "keywords": [
        "nextjs",
        "internet computer",
        "icp",
        "starter",
        "dfinity"
    ],
    "scripts": {
        "dev": "next dev",
        "dev:remote-watch": "next-remote-watch ./_pages",
        "build": "next build && next export",
        "start": "next start",
        "lint": "next lint",
        "export": "next export",
        "syncgencode": "DFX_NETWORK=local && npm run copy:types",
        "copy:types": "rsync -avr .dfx/$(echo ${DFX_NETWORK:-'**'})/canisters/** --exclude='assets/' --exclude='idl/' --exclude='*.wasm' --delete ui/declarations"
    },
    "devDependencies": {
        "@tailwindcss/typography": "0.5.2",
        "autoprefixer": "10.4.0",
        "eslint": "^7.32.0",
        "eslint-config-next": "^11.0.1",
        "eslint-plugin-react": "^7.24.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "postcss": "8.4.5",
        "tailwindcss": "3.0.7"
    },
    "dependencies": {
        "@dfinity/agent": "^0.10.1",
        "@dfinity/auth-client": "^0.10.2",
        "@dfinity/authentication": "0.10.2",
        "@dfinity/candid": "^0.9.2",
        "@dfinity/identity": "0.10.2",
        "@dfinity/principal": "^0.9.2",
        "@headlessui/react": "^1.5.0",
        "@heroicons/react": "^1.0.6",
        "@psychedelic/dab-js": "^1.0.2",
        "@psychedelic/plug-connect": "^0.1.4",
        "@tailwindcss/line-clamp": "^0.3.1",
        "assert": "^2.0.0",
        "buffer": "^6.0.3",
        "canvas-confetti": "1.5.1",
        "classnames": "2.3.1",
        "date-fns": "2.28.0",
        "events": "^3.3.0",
        "formik": "2.2.9",
        "fs": "^0.0.1-security",
        "gray-matter": "4.0.3",
        "next": "12.1.2",
        "next-mdx-remote": "4.0.2",
        "next-remote-watch": "1.0.0",
        "pretty-date": "0.2.0",
        "qrcode.react": "1.0.1",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-google-recaptcha": "^2.1.0",
        "react-linkify": "^1.0.0-alpha",
        "react-query": "3.34.7",
        "react-quill": "^1.3.5",
        "stream-browserify": "^3.0.0",
        "util": "^0.12.4",
        "yup": "0.32.11"
    }
}
